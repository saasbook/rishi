<div class="cmt_name"><%= @name %></div>
    <hr>
    <div class="introduction">
        <p><%= @text %></p>
    </div>
</div>

<% if !current_user.nil? %>
    <% if current_user.role.eql? "internal" or current_user.role.eql? "executive" %>
        <%= puts "my id: "+current_user.uid %>
        <div class="cmt_name"><%= @timeline %></div>
        <hr>
    <% end %>
    <% if current_user.role.eql? "executive" %>
        <%= puts "my id: "+current_user.uid %>
        <%= link_to "Edit Committees", '' %>
    <% end %>
<% end %>