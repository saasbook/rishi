<script src="https://maps.googleapis.com/maps/api/js?key=<%=@api_key%>&callback=initMap"></script>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
<div  class="navigationbar" style='width: 1600px;'>
  <ul  class="navbar">
    <li><%=link_to "GoFundMe!", "https://www.gofundme.com/", id: "Link to GoFundMe!"%></li>
    <li><%=link_to "Members", "/members"%></li>
    <li><%=link_to "About", "/about"%></li>            
  </ul>
</div>

<div style='width: 1600px;'>
  <div id="map" style='width: 1600px; height: 800px;'></div>
</div>

<script type="text/javascript">
var mapOptions = {
  center: new google.maps.LatLng(31.1048,77.1734),
  zoom: 7
};

handler = Gmaps.build('Google');
handler.buildMap({ provider: mapOptions, internal: {id: 'map'}}, function(){
  handler.addMarkers(<%=raw @marker_hash.to_json %>);
});
</script>